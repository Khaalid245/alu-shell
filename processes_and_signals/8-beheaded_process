#!/usr/bin/env bash

# Find the PID of the 7-highlander process
pid=$(pgrep -x 7-highlander)

# Check if the process is running
if [ -n "$pid" ]; then
  # Kill the process
  kill "$pid"
  echo "Successfully killed the 7-highlander process with PID $pid."
else
  echo "The 7-highlander process is not running."
fi

